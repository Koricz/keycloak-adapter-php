{
  "name": "ataccama/keycloak-adapter",
  "description": "Easy-to-use PHP adapter for Keycloak authentication",
  "version": "1.0.0-dev",
  "type": "library",
  "license": [
    "MIT"
  ],
  "require": {
    "php": ">= 7.2.0",
    "nette/utils": "^2.4"
  },
  "require-dev": {
    "nette/tester": "^1.7"
  },
  "autoload": {
    "classmap": ["src/"],
    "files": ["src/bootstrap.php"]
  },
  "minimum-stability": "stable",
  "config": {
    "platform": {
      "php": "7.2"
    }
  }
}
